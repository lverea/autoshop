<!DOCTYPE html>
<html>
  <head>
    <title>
		<% if content_for?(:title) %>
			<%= yield(:title) %>
		<% else %>
			Autoshop
		<% end %>
	</title>
	
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
	<% flash.each do |key, message| %>
		<div><%= message %></div>
	<% end %>	
    <%= yield %>
  </body>
</html>
